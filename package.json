{
  "name": "gold-pulse",
  "version": "2.1.2",
  "description": "Gold Pulse with angular 2",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf ./public/dist",
    "typings": "typings install",
    "postinstall": "npm run typings",
    "compile:scss": "gulp compile:scss",
    "build": "npm run clean && tsc",
    "build_prod": "npm run build && browserify -s main public/dist/main.js > public/dist/bundle.js && npm run minify",
    "minify": "uglifyjs public/dist/bundle.js --compress --mangle --output public/dist/bundle.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MaxBoykoII/gold-pulse-angular2.git"
  },
  "keywords": [
    "node",
    "angular2"
  ],
  "devDependencies": {
    "browserify": "^13.0.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-sass": "^2.3.2",
    "node-sass": "^3.8.0",
    "typescript": "^2.0.2",
    "typings": "1.0.4",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "bower": "^1.7.9",
    "core-js": "^2.4.1",
    "d3": "^4.1.1",
    "es6-shim": "^0.35.1",
    "express": "^4.14.0",
    "lodash": "^4.14.2",
    "reflect-metadata": "^0.1.3",
    "request": "^2.73.0",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "^0.19.38",
    "zone.js": "^0.6.23"
  },
  "author": "Max Boyko <maxboykomax@gmail.com>",
  "license": "ISC"
}
